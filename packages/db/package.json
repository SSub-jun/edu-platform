{
  "name": "@edu-platform/db",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "generate": "prisma generate --schema ./prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema ./prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema ./prisma/schema.prisma",
    "migrate:reset": "prisma migrate reset --schema ./prisma/schema.prisma",
    "validate": "prisma validate --schema ./prisma/schema.prisma",
    "seed": "prisma db seed",
    "studio": "prisma studio --schema ./prisma/schema.prisma",
    "push": "prisma db push --schema ./prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^6.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.0.0",
    "prisma": "^5.22.0",
    "typescript": "^5.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
